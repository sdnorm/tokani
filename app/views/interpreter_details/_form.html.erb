<%= form_with(model: interpreter_detail) do |form| %>
  <%= render "shared/error_messages", resource: form.object %>

  <div class="form-group">
    <%= form.label :interpreter_type %>
    <%= form.number_field :interpreter_type, class: "form-control" %>
  </div>

  <div class="form-group">
    <%= form.label :gender %>
    <%= form.number_field :gender, class: "form-control" %>
  </div>

  <div class="form-group">
    <%= form.label :primary_phone %>
    <%= form.text_field :primary_phone, class: "form-control" %>
  </div>

  <div>
    <%= form.button button_text(form.send(:submit_default_value)), class: "btn btn-primary" %>

    <% if form.object.new_record? %>
      <%= link_to t("cancel"), interpreter_details_path, class: "btn btn-white" %>
    <% else %>
      <%= link_to t("cancel"), interpreter_detail_path(@interpreter_detail), class: "btn btn-white" %>
    <% end %>
  </div>
<% end %>
