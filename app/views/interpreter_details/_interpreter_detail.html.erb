<div id="<%= dom_id interpreter_detail %>">
  <% Array.wrap(@interpreter_detail_fields).each do |field| %>
    <div class="grid md:grid-cols-3 py-5 text-gray-500 border-t border-gray-200">
      <div>
        <p><%= field.titleize %></p>
      </div>
      <div class="col-span-2 text-gray-800 capitalize"><%= interpreter_detail.send(field) %></div>
    </div>
  <% end %>
</div>
