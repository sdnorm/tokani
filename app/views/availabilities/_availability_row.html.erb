<tr id="<%= availability.id %>_form_wrapper" class= "">
  <td class="whitespace-nowrap py-4 pl-4 text-sm text-gray-500"><%= start_avail(availability) %></td>
  <td class="whitespace-nowrap px-3 py-4 text-sm text-gray-500"><%= end_avail(availability) %></td>
  <td class="whitespace-nowrap px-3 py-4 text-sm text-gray-500">
    <% if availability.in_person %>
      <i class="fa-solid fa-square-check">
    <% end %>
  </td>
  <td class="whitespace-nowrap px-3 py-4 text-sm text-gray-500">
    <% if availability.phone %>
      <i class="fa-solid fa-square-check">
    <% end %>
  </td>
  <td class="whitespace-nowrap px-3 py-4 text-sm text-gray-500">
    <% if availability.video %>
      <i class="fa-solid fa-square-check">
    <% end %>
  </td>
  <td class="whitespace-nowrap px-3 py-4">
    <%= button_to url_for(controller: "availabilities", action: "destroy", id: availability.id), params: {'availability[interpreter_id]' => @interpreter.id}, method: :delete, class:"rounded-2 border-blueCoral me-2 mt-2" do %>
      <i class="fa fa-xl fa-trash"></i>
    <% end %>
  </td>
</tr>