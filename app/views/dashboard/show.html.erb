<% if current_account_user.roles["interpreter"] %>
  <%= render template: "interpreters/index" %>
<% elsif current_account_user.agency_admin? %>
  <%= render template: "agency_admin" %>
<% elsif current_account_user.customer_admin? %>
  <%= render partial: "customer" %>
<% elsif current_account_user.admin? %>
  <%= render partial: "tokani_admin" %>
<% else %>
  <%= render partial: "general" %>
<% end %>